{
  "name": "Vocabular.io Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "key": "hgmkilegjimffhfpkajmmcpnhjapmfef",
  "description": "This is the extension for vocabular.io!",
  "homepage_url": "http://dhchoi.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.min.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "the browser action",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "contextMenus",
    "notifications",
    "storage",
    "tabs",
    "identity",
    "https://www.googleapis.com/*",
    "https://ssl.gstatic.com/",
    "https://accounts.google.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ],
  "oauth2": {
    "client_id": "803788324725-h7obj1sku5c4gs40u2h5ud7f112vas1e.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/plus.login"
    ]
  }
}
